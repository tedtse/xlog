<div role="xlog-head">
  <h3 class="clearfix" role="xlog-titlebar">XLog
    <a class="close" role="xlog-close-button" href="javascript:">x</a>
  </h3>
  <div class="xlog-taskbar" role="xlog-taskbar">
    <a class="xlog-clean-button" href="javascript:;" role="xlog-clean-button"></a>
    <a class="xlog-filter-button" href="javascript:;" role="xlog-filter-button"></a>
  </div>
  <div class="xlog-dropbar" role="xlog-dropbar" style="display: none">
    <a class="xlog-filter-all selected" href="javascript:;" role="xlog-filter-all">All</a>
    <a class="xlog-filter-error" href="javascript:;" role="xlog-filter-error">Errors</a>
    <a class="xlog-filter-warn" href="javascript:;" role="xlog-filter-warn">Warnings</a>
    <a class="xlog-filter-info" href="javascript:;" role="xlog-filter-info">Info</a>
    <a class="xlog-filter-log" href="javascript:;" role="xlog-filter-log">Logs</a>
    <a class="xlog-filter-debug" href="javascript:;" role="xlog-filter-debug">Debug</a>
  </div>
</div>
<div class="xlog-body" role="xlog-body">
  <ul role="xlog-entity"></ul>
</div>